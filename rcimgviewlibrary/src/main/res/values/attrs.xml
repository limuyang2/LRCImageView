<?xml version="1.0" encoding="utf-8"?>
<resources>

	<declare-styleable name="RCImageView">
		<attr name="rc_borderRadius" format="dimension"/>
		<attr name="rc_type">
			<enum name="normal" value="-1"/>
			<enum name="circle" value="0"/>
			<enum name="round" value="1"/>
		</attr>
		<attr name="rc_borderWidth" format="dimension" />
		<attr name="rc_borderColor" format="color" />
	</declare-styleable>

	<declare-styleable name="RoundImageView">
		<attr name="rc_round_leftUp" format="dimension"/>
		<attr name="rc_round_leftDown" format="dimension"/>
		<attr name="rc_round_rightUp" format="dimension"/>
		<attr name="rc_round_rightDown" format="dimension"/>
		<attr name="rc_round_corner" format="dimension"/>
	</declare-styleable>

</resources>